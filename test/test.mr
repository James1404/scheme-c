(import "lib/std/comparison.mr")

(define factorial (lambda (n)
    (if (= n 0)
        1
        (* n (factorial (- n 1))))))

(define dec (lambda (x) (- x 1)))
(define inc (lambda (x) (+ x 1)))

(define fib (lambda (n)
    (if (<= n 2)
        1
        (+ (fib (dec n)) (fib (- n 2))))
    ))

