(import "lib/std/comparison.mr")

(def factorial (fn (n)
    (if (= n 0)
        1
        (* n (factorial (- n 1))))))

(def dec (fn (x) (- x 1)))
(def inc (fn (x) (+ x 1)))

(def fib (fn (n)
    (if (<= n 2)
        1
        (+ (fib (dec n)) (fib (- n 2))))
    ))

(println (fib 20))

(def ok "ok")
(println ok)
